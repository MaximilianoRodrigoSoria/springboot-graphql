type Cliente
    {
        nombre:String
        apellido:String
        email:String
    }

extend type Query {
    getCliente(id:Int):Cliente
    getClientes:[Cliente]
    
}

extend type Mutation {
    saveCliente(nombre:String , apellido:String , email:String ):Cliente
    updateCliente(id:Int, nombre:String , apellido:String , email:String ):Cliente
    deleteCliente(id:Int):Cliente
}




